{
    "name": "ericmakesstuff/display-name",
    "description": "A simple package to show user's real names in a variety of Web-safe ways, i.e. Eric B. or E B.",
    "keywords": [
        "EricMakesStuff",
        "DisplayName",
        "Initials"
    ],
    "homepage": "https://github.com/ericmakesstuff/display-name",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Blount",
            "email": "eric@ericmakesstuff.com",
            "homepage": "http://ericmakesstuff.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "EricMakesStuff\\DisplayName\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EricMakesStuff\\DisplayName\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
